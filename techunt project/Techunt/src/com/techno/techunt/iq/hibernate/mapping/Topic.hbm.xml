<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.techno.techunt.iq.hibernate.pc">
<class name="Topic" table="TOPIC_TABLE">
<id name="topicid" column="TOPIC_ID" type="int">
<generator class="increment"/>
</id>
<property name="topicname" column="TOPIC_NAME"/>
<property name="numberofquestion" column="NUMBER_OF_QUESTION"/>
</class>
</hibernate-mapping>
