<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>FilterStudy</display-name>
  <welcome-file-list>
    <welcome-file>hello.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>JLCFilter</filter-name>
    <filter-class>com.jlcindia.filters.JLCFilterReq</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>JLCFilter</filter-name>
    <url-pattern>/hello.jlc</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>JLCFilterRES</filter-name>
    <filter-class>com.jlcindia.filters.JLCFilterRes</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>JLCFilterRES</filter-name>
    <url-pattern>/hellores.jlc</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>HelloServlet</display-name>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>com.jlcindia.servlets.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hello.sau</url-pattern>
  </servlet-mapping>
</web-app>