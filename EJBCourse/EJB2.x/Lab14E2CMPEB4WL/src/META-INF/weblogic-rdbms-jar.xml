<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE weblogic-rdbms-jar PUBLIC 
"-//BEA Systems, Inc.//DTD WebLogic 8.1.0 EJB RDBMS Persistence//EN"
'http://www.bea.com/servers/wls810/dtd/weblogic-rdbms20-persistence-810.dtd'>



<weblogic-rdbms-jar>
<weblogic-rdbms-bean>
<ejb-name>AccountEJB</ejb-name>
<data-source-name>MySQLDataSourceJNDI</data-source-name>
<table-map>
<table-name>account</table-name>
<field-map>
<cmp-field>accno</cmp-field>
<dbms-column>accno</dbms-column>
</field-map>
<field-map>
<cmp-field>atype</cmp-field>
<dbms-column>atype</dbms-column>
</field-map>
<field-map>
<cmp-field>branch</cmp-field>
<dbms-column>branch</dbms-column>
</field-map>
<field-map>
<cmp-field>bal</cmp-field>
<dbms-column>bal</dbms-column>
</field-map>
</table-map>
</weblogic-rdbms-bean>
</weblogic-rdbms-jar>