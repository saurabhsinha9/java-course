create table jlcbooks(
bookId char(6) primary key,
bookName char(15),
author char(15),
pub char(15),
isbn char(15),
cost double,
cat char(15),
qty int);

 insert into jlcbooks values('1','Java','Sri','JLC','12-13',500.0,'java',1);
